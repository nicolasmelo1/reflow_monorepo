{
  "name": "client",
  "version": "0.0.0",
  "description": "\"monorepo for the client of reflow, will hold the mobile, desktop and web versions of the app\"",
  "main": "index.js",
  "scripts": {
    "uninstall:desktop": "npm uninstall --prefix desktop",
    "uninstall:web": "npm uninstall --prefix web",
    "uninstall:mobile": "npm uninstall --prefix mobile",
    "install:desktop": "npm install --prefix desktop",
    "install:web": "npm install --prefix web",
    "install:mobile": "npm install --prefix mobile",
    "desktop:dev": "npm run dev --prefix desktop",
    "web:dev": "npm run dev --prefix web",
    "web:build": "npm run build --prefix web",
    "web:start": "npm run start --prefix web",
    "mobile:start": "npm run start --prefix mobile"
  },
  "keywords": [
    "reflow",
    "client"
  ],
